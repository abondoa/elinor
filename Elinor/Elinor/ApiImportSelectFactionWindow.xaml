<Window x:Class="Elinor.ApiImportSelectFactionWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Title="Select corp and faction" Height="194" Width="409"
        ResizeMode="CanMinimize" Loaded="WindowLoaded" Icon="/Elinor;component/Images/mainicon.png">
  <Grid>
    <Label Content="Corporation" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1"
           VerticalAlignment="Top" />
    <Label Content="Faction" Height="28" HorizontalAlignment="Left" Margin="12,46,0,0" Name="label2"
           VerticalAlignment="Top" />
    <ComboBox Height="23" HorizontalAlignment="Left" Margin="104,15,0,0" Name="cbCorp" VerticalAlignment="Top"
              Width="271" />
    <ComboBox Height="23" HorizontalAlignment="Left" Margin="104,49,0,0" Name="cbFaction" VerticalAlignment="Top"
              Width="271" />
    <Button Content="OK" Height="23" HorizontalAlignment="Right" Margin="0,0,12,12" Name="btnOk"
            VerticalAlignment="Bottom" Width="75" Click="BtnOkClick" />
  </Grid>
</Window>